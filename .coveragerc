[run]
source = maas_cpu_analyzer
omit =
    */tests/*
    */test_*
    */__pycache__/*
    */venv/*
    */build/*
    */dist/*
    */site-packages/*
    setup.py
    */conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
    # Have to re-enable the standard pragma
    pragma: no cover

[html]
directory = htmlcov
